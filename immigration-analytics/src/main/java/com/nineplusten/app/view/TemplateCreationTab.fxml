<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="800.0" prefWidth="1200.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nineplusten.app.view.TemplateCreationController">
    <children>
        <VBox layoutX="506.0" layoutY="501.0" prefHeight="180.0" prefWidth="188.0" spacing="10.0">
        <children>
            <Label text="Select Template's Agency" />
            <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdate" prefWidth="150.0" />
            <Label fx:id="comboBoxLabel" text="Agency Name" />
            <TextField fx:id="AgencyNameText" />
        </children>
        </VBox>
      <TableView fx:id="templateTable" layoutX="215.0" layoutY="90.0" prefHeight="294.0" prefWidth="810.0" />
      <Button layoutX="215.0" layoutY="52.0" mnemonicParsing="false" onAction="#addColumn" text="+" />
      <Button layoutX="260.0" layoutY="52.0" mnemonicParsing="false" onAction="#delColumn" prefHeight="26.0" prefWidth="29.0" text="-" />
    </children>
</AnchorPane>
