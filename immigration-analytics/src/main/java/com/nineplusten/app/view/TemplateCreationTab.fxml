<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="550.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nineplusten.app.view.TemplateCreationController">
    <children>
        <VBox layoutX="386.0" layoutY="328.0" prefHeight="180.0" prefWidth="188.0" spacing="10.0">
        <children>
            <Label text="Select Template's Agency" />
            <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdate" prefWidth="150.0" />
            <Label fx:id="comboBoxLabel" text="Agency Name" />
            <TextField fx:id="AgencyNameText" />
        </children>
        </VBox>
      <Label layoutX="450.0" layoutY="139.0" text="OR" />
      <ListView fx:id="colList" layoutX="27.0" layoutY="27.0" prefHeight="480.0" prefWidth="162.0" />
      <TextField fx:id="newColName" layoutX="207.0" layoutY="77.0" text="add new col" />
      <Button layoutX="367.0" layoutY="77.0" mnemonicParsing="false" onAction="#listviewAdd" text="add" />
      <Button layoutX="207.0" layoutY="36.0" mnemonicParsing="false" onAction="#listviewRemove" text="remove selected column name" />
    </children>
</AnchorPane>
